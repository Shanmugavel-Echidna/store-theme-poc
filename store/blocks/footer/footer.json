{
  "footer": {
    "blocks": [
      "footer-layout.desktop"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1",
      "flex-layout.row#footer-bottom"
    ]
  },
  "flex-layout.row#footer-1": {
    "props": {
      "blockClass": "footer-row",
      "paddingTop": 6,
      "paddingBottom": 6
    },
    "children": [
      "image#logo",
      "flex-layout.col#collections-menu",
      "flex-layout.col#resources-menu",
      "flex-layout.col#contact-menu"
    ]
  },
  "flex-layout.col#collections-menu": {
    "children": [
      "rich-text#collections-title",
      "vtex.menu@2.x:menu#collections"
    ],
    "props": {
      "blockClass": "footer-menu-column"
    }
  },
  "flex-layout.col#resources-menu": {
    "children": [
      "rich-text#resources-title",
      "vtex.menu@2.x:menu#resources"
    ],
    "props": {
      "blockClass": "footer-menu-column"
    }
  },
  "flex-layout.col#contact-menu": {
    "children": [
      "rich-text#contact-title",
      "vtex.menu@2.x:menu#contact"
    ],
    "props": {
      "blockClass": "footer-menu-column"
    }
  },
  "flex-layout.row#footer-bottom": {
    "props": {
      "blockClass": "footer-bottom"
    },
    "children": []
  },
 "image#logo": {
  "props": {
    "src": "https://parts.nissanusa.com/images/nissan-oem-assets/icons/social-youtube-white.png",
    "alt": "American Leather Logo",
    "blockClass": "footer-logo"
  }
},
  "rich-text#collections-title": {
    "props": {
      "text": "### COLLECTIONS",   
      "blockClass": "footer-menu-title"
    }
  },
  "rich-text#resources-title": {
    "props": {
      "text": "### RESOURCES",
      "blockClass": "footer-menu-title"
    }
  },
  "rich-text#contact-title": {
    "props": {
      "text": "### CONTACT US",
      "blockClass": "footer-menu-title"
    }
  }
}